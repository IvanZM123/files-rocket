{
  "name": "@filesrocket/local",
  "version": "0.0.0",
  "description": "Filesrocket service that manages your files locally",
  "main": "lib/index.js",
  "types": "lib",
  "author": {
    "name": "Ivan Zaldivar",
    "url": "https://github.com/thebug404"
  },
  "repository": {
    "url": "https://github.com/Filesrocket/filesrocket"
  },
  "bugs": {
    "url": "https://github.com/Filesrocket/filesrocket/issues"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**",
    "README.md",
    "LICENCE"
  ],
  "keywords": [
    "filemanager",
    "filemanagement",
    "upload",
    "storage",
    "files"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && npm run test && tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "release": "standard-version",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@filesrocket/core": "*"
  },
  "devDependencies": {}
}
